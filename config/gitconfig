[alias]
    a = add
    aa = add --all
    ci = commit -a
    st = status
    unstage = reset HEAD
    undo-commit = reset --soft HEAD^
[color]
    ui = auto
[core]
    editor = nvim
[init]
	defaultBranch = main
[pull]
	rebase = false
[user]
	email = malteut@gmail.com
    name = malte
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
