#! /bin/sh

/home/malte/dotfiles/local/screenlayout.sh

xset r rate 200 25
xautolock -time 25 -locker blurlock &

# KEYBOARD LAYOUT
setxkbmap -layout gfkl -variant wokmok_lshift \
    -model pc105 -option altwin:swap_lalt_lwin &
xcape -e '#66=Escape;#23=Tab' -t 180 &

gitwatch ~/repos/vimwiki &

exec bspwm
